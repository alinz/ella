ella="0.0.1"

enum Flag int32 { 
  NoName = 1 
  WithName 
}

message User { 
  ID: string
  Name: string
  Flag: Flag {
    json = flag
  }
  CreatedOn: timestamp
}

# awesome service
service GreetingService {
	Ping(userId: string)
  GetUsers() => (users: []User)
	StatusStream(userId: string) => (users: stream User, guests: stream User)
}